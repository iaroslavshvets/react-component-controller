{
  "name": "react-component-controller",
  "version": "10.0.0",
  "license": "UNLICENSED",
  "author": {
    "name": "Iaroslav Shvets",
    "email": "iaroslavs@wix.com"
  },
  "description": "Controller for managing component state React applications",
  "homepage": "https://github.com/iaroslavshvets/react-component-controller",
  "repository": {
    "type": "git",
    "url": "git@github.com:iaroslavshvets/react-component-controller.git",
    "directory": "/react-controller"
  },
  "scripts": {
    "start": "yoshi start",
    "build": "yoshi build",
    "test": "npm run test:unit",
    "test:watch": "yoshi test --watch",
    "test:unit": "yoshi test",
    "lint": "yoshi lint --fix"
  },
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**",
    "src/**",
    "statics/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,scss,css}": "yoshi lint --fix"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/chai": "^4.3.0",
    "@types/history": "^4.7.8",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.6",
    "@wix/yoshi": "^4.318.0",
    "chai": "^4.3.4",
    "eslint-config-yoshi": "^4.78.0",
    "husky": "^7.0.4",
    "jest-yoshi-preset": "^4.87.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^12.0.1",
    "typescript": "^4.5.4"
  },
  "sideEffects": false,
  "jest": {
    "preset": "jest-yoshi-preset",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "yoshi": {
    "entry": {
      "index": "./index.ts"
    },
    "externals": {
      "react": {
        "commonjs": "react",
        "commonjs2": "react",
        "amd": "react",
        "root": "React"
      },
      "react-dom": {
        "commonjs": "react-dom",
        "commonjs2": "react-dom",
        "amd": "reactDOM",
        "root": "ReactDOM"
      }
    }
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  }
}
