{
  "name": "react-component-controller",
  "version": "11.4.5",
  "license": "UNLICENSED",
  "author": {
    "name": "Iaroslav Shvets",
    "email": "iaroslavs@wix.com"
  },
  "description": "Controller for managing component state React applications",
  "homepage": "https://github.com/iaroslavshvets/react-component-controller",
  "repository": {
    "type": "git",
    "url": "git@github.com:iaroslavshvets/react-component-controller.git",
    "directory": "/react-controller"
  },
  "scripts": {
    "build": "rollup --config",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:ci-coverage": "jest --ci --coverage",
    "lint": "yarn es-lint && yarn format",
    "es-lint": "eslint --fix '{src,__tests__}/**/*.{ts,tsx}'",
    "format": "prettier --write '{src,__tests__}/**/*.{ts,tsx,js,json,scss,css}'"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist/**"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,scss,css}": "yoshi lint --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@swc/core": "^1.3.71",
    "@swc/jest": "^0.2.27",
    "@testing-library/react": "^14.0.0",
    "@types/chai": "^4.3.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "6.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-isaacscript": "^2.6.7",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "jest-cli": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.3",
    "rollup-plugin-size": "^0.3.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sinon": "^15.1.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "sideEffects": false,
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
