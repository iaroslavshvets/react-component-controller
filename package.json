{
  "name": "react-component-controller",
  "version": "6.0.3",
  "license": "UNLICENSED",
  "author": {
    "name": "Iaroslav Shvets",
    "email": "iaroslavs@wix.com"
  },
  "description": "Controller for managing component state React applications",
  "homepage": "https://github.com/iaroslavshvets/react-component-controller",
  "repository": {
    "type": "git",
    "url": "git@github.com:iaroslavshvets/react-component-controller.git",
    "directory": "/react-controller"
  },
  "scripts": {
    "build": "yoshi-library build",
    "test": "npm run test:unit",
    "test:watch": "yoshi-library test --watch",
    "test:unit": "yoshi-library test",
    "lint": "yoshi-library lint --fix"
  },
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**",
    "src/**",
    "statics/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,scss,css}": "yoshi lint --fix"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/chai": "^4.2.21",
    "@types/history": "^4.7.9",
    "@types/node": "^16.4.2",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "husky": "^7.0.1",
    "jest-yoshi-preset": "^5.1.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^11.1.1",
    "typescript": "^4.3.5",
    "yoshi-flow-library": "^5.1.0"
  },
  "sideEffects": false,
  "jest": {
    "preset": "jest-yoshi-preset",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "versionBumpStrategy": "patch"
  },
  "yoshiFlowLibrary": {
    "bundle": {
      "entry": {
        "index": "./index.ts"
      },
      "externals": {
        "react": {
          "commonjs": "react",
          "commonjs2": "react",
          "amd": "react",
          "root": "React"
        },
        "react-dom": {
          "commonjs": "react-dom",
          "commonjs2": "react-dom",
          "amd": "reactDOM",
          "root": "ReactDOM"
        }
      }
    }
  },
  "dependencies": {}
}
